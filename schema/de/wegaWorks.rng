                </sch:rule>
         </pattern>
         <ref name="att.id.attribute.xmlid"/>
         <ref name="att.typed.attribute.type"/>
         <ref name="att.classed.attribute.class"/>
         <ref name="att.bibl.attributes"/>
         <empty/>
