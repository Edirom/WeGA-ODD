   <define name="date">
      <element name="date" ns="http://www.music-encoding.org/ns/mei">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A string identifying a point in time or the time period between two such points.</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="model.textphraseLike"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="wegaWorks-date-dateHeuristics1-constraint-report-1">
            <rule context="mei:date">
               <sch:report xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           test="(@notbefore gt @notafter) and not(starts-with(@notbefore, '-'))">value of notBefore greater than notAfter</sch:report>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="wegaWorks-date-dateHeuristics3-constraint-report-2">
            <rule context="mei:date">
               <sch:report xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           test="(@startdate or @enddate) and (@isodate or @notafter or @notbefore)">cooccurence of date and duration attributes</sch:report>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="wegaWorks-date-dateHeuristics4-constraint-report-3">
            <rule context="mei:date">
               <sch:report xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           test="@isodate and (@notafter or @notbefore)">cooccurence of exact and uncertain date attributes</sch:report>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="wegaWorks-date-dateHeuristics5-constraint-assert-31">
            <rule context="mei:date">
               <sch:assert xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           test="@isodate or @notbefore or @notafter or @startdate or @enddate or @cert='unknown' or matches(., '0000|o\.J\.|unknown')">mei:date benötigt ein Attribut @isodate, @notbefore o.ä.</sch:assert>
            </rule>
         </pattern>
         <ref name="att.calendared.attributes"/>
         <ref name="att.datable.attributes"/>
         <ref name="att.edit.attributes"/>
         <ref name="att.typed.attributes"/>
         <empty/>
