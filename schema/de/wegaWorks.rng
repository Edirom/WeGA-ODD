         <ref name="model.relationLike_sequenceRepeatable"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="model.rendLike_sequenceRepeatable"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="model.titleLike_sequenceRepeatable"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="dimensions"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="extent"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="term"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="model.qLike_sequenceRepeatable"/>
      </oneOrMore>
   </define>
   <define name="model.titleLike">
