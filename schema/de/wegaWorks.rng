   <define name="change">
      <element name="change">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Individual change within the revision description.</a:documentation>
         <optional>
            <ref name="respStmt"/>
         </optional>
         <ref name="changeDesc"/>
         <optional>
            <ref name="model.dateLike"/>
         </optional>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="wegaWorks-change-change_requirements-constraint-rule-32">
            <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
