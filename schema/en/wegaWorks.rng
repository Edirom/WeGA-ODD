  <define name="staffDef">
    <element name="staffDef">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(staff definition) â€“ Container for staff meta-information.</a:documentation>
      <zeroOrMore>
        <ref name="model.labelLike"/>
