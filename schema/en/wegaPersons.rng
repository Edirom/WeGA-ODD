   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="wegaPersons-att.calendarSystem-calendar_attr_on_empty_element-constraint-rule-8">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:xi="http://www.w3.org/2001/XInclude"
                context="tei:*[@calendar]">
         <sch:assert test="string-length( normalize-space(.) ) gt 0"> @calendar indicates one or more
              systems or calendars to which the date represented by the content of this element belongs,
              but this <sch:name/> element has no textual content.</sch:assert>
      </sch:rule>
   </pattern>
   <define name="p">
      <element name="p">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(paragraph) marks paragraphs in prose. [3.1. Paragraphs 7.2.5. Speech Contents]</a:documentation>
         <ref name="macro.paraContent"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-p-extent-within-physDesc-constraint-rule-9">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:p">
               <report xmlns="http://purl.oclc.org/dsdl/schematron"
                       test="@n='extent' and not(parent::tei:physDesc)">
                            The value 'extent' must only be used within the tei:physDesc.
                        </report>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-p-extent-only-once-constraint-rule-10">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:physDesc">
               <sch:report test="count(tei:p[@n='extent']) gt 1">There must only be one tei:p with @n='extent'</sch:report>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-p-extent-regex-constraint-rule-11">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:p[@n='extent']">
               <sch:assert test="matches(., '^\d+ D?Bl\. \(\d+ b\. S\.( (o|einschl)\. Adr\.)?( u\. Umschlag)?\)$')">The 'extent' must match the pattern '^\d+ D?Bl\. \(\d+ b\. S\.( (o|einschl)\. Adr\.)?( u\. Umschlag)?\)$'</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-p-abstractModel-structure-p-in-ab-or-p-constraint-rule-12">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:p">
               <report xmlns="http://purl.oclc.org/dsdl/schematron"
                       test="(ancestor::tei:p or ancestor::tei:ab)                              and not(parent::tei:exemplum                             |parent::tei:item                             |parent::tei:note                             |parent::tei:q                             |parent::tei:quote                             |parent::tei:remarks                             |parent::tei:said                             |parent::tei:sp                             |parent::tei:stage                             |parent::tei:cell                             |parent::tei:figure                             |parent::tei:footNote)">
                            Abstract model violation: Paragraphs may not contain other paragraphs or ab elements.
                        </report>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-p-abstractModel-structure-l-constraint-rule-13">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:p">
               <report xmlns="http://purl.oclc.org/dsdl/schematron"
                       test="(ancestor::tei:l or ancestor::tei:lg)                              and not(parent::tei:figure or parent::tei:note or ancestor::tei:floatingText or parent::tei:footNote)">
                            Abstract model violation: Lines may not contain higher-level structural elements such as div, p, or ab, unless p is a child of figure or note, or is a descendant of floatingText.
                        </report>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-p-abstractModel-structure-p-in-l-constraint-rule-14">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:l//tei:p">
               <sch:assert test="ancestor::tei:floatingText | parent::tei:figure | parent::tei:note">
          Abstract model violation: Metrical lines may not contain higher-level structural elements such as div, p, or ab, unless p is a child of figure or note, or is a descendant of floatingText.
        </sch:assert>
            </sch:rule>
         </pattern>
         <ref name="att.fragmentable.attributes"/>
         <ref name="att.written.attributes"/>
         <optional>
            <attribute name="n">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(number) gives a number (or other label) for an element, which is not necessarily unique within the document.</a:documentation>
               <choice>
                  <value>1</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks the (logical) first paragraph of some division of text.</a:documentation>
                  <value>extent</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks the paragraph describing the extent of a manuscript.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="rend">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(rendition) indicates how the element in question was rendered or presented in the source text.</a:documentation>
               <choice>
                  <value>inline</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks an inline paragraph</a:documentation>
               </choice>
            </attribute>
         </optional>
         <ref name="att.global.attribute.xmlid"/>
         <empty/>
      </element>
   </define>
   <define name="hi">
      <element name="hi">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(highlighted) marks a word or phrase as graphically distinct from the surrounding text, for reasons concerning which no claim is made. [3.3.2.2. Emphatic Words and Phrases 3.3.2. Emphasis, Foreign Words, and Unusual Language]</a:documentation>
         <ref name="macro.paraContent"/>
         <ref name="att.wega.rend.attributes"/>
         <ref name="att.written.attributes"/>
         <ref name="att.global.attribute.xmlid"/>
         <empty/>
      </element>
   </define>
   <define name="quote">
      <element name="quote">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(quotation) contains a phrase or passage attributed by the narrator or author to some agency external to the text. [3.3.3. Quotation 4.3.1. Grouped Texts]</a:documentation>
         <ref name="macro.specialPara"/>
         <ref name="att.global.source.attributes"/>
         <ref name="att.notated.attributes"/>
         <optional>
            <attribute name="rend">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies how the the quotation is typographically distinct from the surrounding text. When no <code xmlns="http://www.w3.org/1999/xhtml"
                        xmlns:rng="http://relaxng.org/ns/structure/1.0">@rend</code> is given, the quotation is not visible.</a:documentation>
               <choice>
                  <value>double-quotes</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">the quotation is surrounded by double quotation marks.</a:documentation>
                  <value>single-quotes</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">the quotation is surrounded by single quotation marks.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="q">
      <element name="q">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(quoted) contains material which is distinguished from the surrounding text using quotation marks. [3.3.3. Quotation]</a:documentation>
         <ref name="macro.specialPara"/>
         <ref name="att.ascribed.directed.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(type) may be used to indicate whether the offset passage is spoken or thought, or to characterize it more finely.
Suggested values include: 1] spoken (spoken); 2] thought (thought); 3] written (written); 4] soCalled (so called); 5] foreign (foreign); 6] distinct (distinct); 7] term; 8] emph (emph); 9] mentioned (mentioned)</a:documentation>
               <choice>
                  <value>spoken</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(spoken) representation of speech</a:documentation>
                  <value>thought</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(thought) representation of thought, e.g. internal monologue</a:documentation>
                  <value>written</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(written) quotation from a written source</a:documentation>
                  <value>soCalled</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(so called) authorial distance</a:documentation>
                  <value>foreign</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(foreign) foreign words</a:documentation>
                  <value>distinct</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(distinct) linguistically distinct</a:documentation>
                  <value>term</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">technical term</a:documentation>
                  <value>emph</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(emph) rhetorically emphasized</a:documentation>
                  <value>mentioned</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(mentioned) refering to itself, not its normal referent</a:documentation>
                  <data type="token">
                     <param name="pattern">[^\p{C}\p{Z}]+</param>
                  </data>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="sic">
      <element name="sic">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Latin for thus or so) contains text reproduced although apparently incorrect or inaccurate. [3.5.1. Apparent Errors]</a:documentation>
         <ref name="macro.paraContent"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-sic-empty-sic-constraint-rule-15">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:sic">
               <sch:report test="normalize-space(.) = ''">ein sic sollte immer Text enthalten. Alternativ kann ein supplied oder gap eingesetzt werden.</sch:report>
            </sch:rule>
         </pattern>
      </element>
   </define>
   <define name="corr">
      <element name="corr">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(correction) contains the correct form of a passage apparently erroneous in the copy text. [3.5.1. Apparent Errors]</a:documentation>
         <ref name="macro.paraContent"/>
      </element>
   </define>
   <define name="choice">
      <element name="choice">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(choice) groups a number of alternative encodings for the same point in a text. [3.5. Simple Editorial Changes]</a:documentation>
         <choice>
            <group>
               <ref name="sic"/>
               <ref name="corr"/>
            </group>
            <empty/>
         </choice>
      </element>
   </define>
   <define name="add">
      <element name="add">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(addition) contains letters, words, or phrases inserted in the source text by an author, scribe, annotator, or corrector. [3.5.3. Additions, Deletions, and Omissions]</a:documentation>
         <ref name="macro.paraContent"/>
         <ref name="att.editLike.attribute.evidence"/>
         <ref name="att.editLike.attribute.instant"/>
         <ref name="att.written.attribute.hand"/>
         <attribute name="place">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies where this item is placed.</a:documentation>
            <list>
               <oneOrMore>
                  <choice>
                     <value>above</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">insertion above the line.</a:documentation>
                     <value>below</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">insertion below the line.</a:documentation>
                     <value>inline</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">insertion inline.</a:documentation>
                     <value>margin</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">insertion at margin.</a:documentation>
                     <value>mixed</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Additions at mixed places.</a:documentation>
                     <value>overleaf</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Addition on the other side of the leaf.</a:documentation>
                  </choice>
               </oneOrMore>
            </list>
         </attribute>
         <ref name="att.global.attribute.xmlid"/>
         <empty/>
      </element>
   </define>
   <define name="del">
      <element name="del">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(deletion) contains a letter, word, or passage deleted, marked as deleted, or otherwise indicated as superfluous or spurious in the copy text by an author, scribe, annotator, or corrector. [3.5.3. Additions, Deletions, and Omissions]</a:documentation>
         <ref name="macro.paraContent"/>
         <attribute name="rend">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(rendition) indicates how the element in question was rendered or presented in the source text.</a:documentation>
            <choice>
               <value>strikethrough</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(crossed out) The deleted word (or the deleted character) is crossed out.</a:documentation>
               <value>overwritten</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(overwritten) The deleted word (or the deleted character) is overwritten by the substitution.</a:documentation>
               <value>erased</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(covered) The deleted word (or the deleted character) is repaid by shaving or other means.</a:documentation>
            </choice>
         </attribute>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.written.attribute.hand"/>
         <empty/>
      </element>
   </define>
   <define name="rs">
      <element name="rs">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(referencing string) identifies one or more objects by type person, work, dairy entry, and so on. [14.2.1. Personal Names 3.6.1. Referring Strings]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-rs-rs-typeNkey-constraint-rule-16">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='person'][@key]">
               <sch:assert test="matches(@key, '^A00[0-9A-F]{4}$')">A type value of 'person' must have only one key, starting with 'A00'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='persons'][@key]">
               <sch:assert test="matches(@key, '^(A00[0-9A-F]{4}\s)*A00[0-9A-F]{4}$')">A type value of 'persons' must have at least one key, each starting with 'A00'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='work'][@key]">
               <sch:assert test="matches(@key, '^A02\d{4}$')">A type value of 'work' must have only one key, starting with 'A02'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='works'][@key]">
               <sch:assert test="matches(@key, '^(A02\d{4}\s)*A02\d{4}$')">A type value of 'works' must have at least one key, each starting with 'A02'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='writing'][@key]">
               <sch:assert test="matches(@key, '^A03\d{4}$')">A type value of 'writing' must have only one key, starting with 'A03'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='writings'][@key]">
               <sch:assert test="matches(@key, '^(A03\d{4}\s)*A03\d{4}$')">A type value of 'writings' must have at least one key, each starting with 'A03'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='letter'][@key]">
               <sch:assert test="matches(@key, '^A04\d{4}$')">A type value of 'letter' must have only one key, starting with 'A04'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='letters'][@key]">
               <sch:assert test="matches(@key, '^(A04\d{4}\s)*A04\d{4}$')">A type value of 'letters' must have at least one key, each starting with 'A04'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='news'][@key]">
               <sch:assert test="matches(@key, '^(A05\d{4}\s)*A05\d{4}$')">A type value of 'news' must have at least one key, starting with 'A05'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='diaryDay'][@key]">
               <sch:assert test="matches(@key, '^A06\d{4}$')">A type value of 'diaryDay' must have only one key, starting with 'A06'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='diaryDays'][@key]">
               <sch:assert test="matches(@key, '^(A06\d{4}\s)*A06\d{4}$')">A type value of 'diaryDays' must have at least one key, each starting with 'A06'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='org'][@key]">
               <sch:assert test="matches(@key, '^A08\d{4}$')">A type value of 'org' must have only one key, starting with 'A08'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='orgs'][@key]">
               <sch:assert test="matches(@key, '^(A08\d{4}\s)*A08\d{4}$')">A type value of 'orgs' must have at least one key, each starting with 'A08'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='document'][@key]">
               <sch:assert test="matches(@key, '^A10\d{4}$')">A type value of 'document' must have only one key, starting with 'A10'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='documents'][@key]">
               <sch:assert test="matches(@key, '^(A10\d{4}\s)*A10\d{4}$')">A type value of 'documents' must have at least one key, each starting with 'A10'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='biblio'][@key]">
               <sch:assert test="matches(@key, '^(A11\d{4}\s)*A11\d{4}$')">A type value of 'biblio' must have at least one key, each starting with 'A11'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='addenda'][@key]">
               <sch:assert test="matches(@key, '^A12\d{4}$')">A type value of 'addenda' must have only one key, starting with 'A12'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='place'][@key]">
               <sch:assert test="matches(@key, '^A13\d{4}$')">A type value of 'place' must have only one key, starting with 'A13'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='places'][@key]">
               <sch:assert test="matches(@key, '^(A13\d{4}\s)*A13\d{4}$')">A type value of 'places' must have at least one key, each starting with 'A13'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:rs[@type='mixed'][@key]">
               <sch:assert test="count(distinct-values(for $i in tokenize(@key, '\s+') return substring($i, 1, 3))) gt 1">A type value of 'mixed' must have keys referring to different document types</sch:assert>
            </sch:rule>
         </pattern>
         <ref name="att.typed.attribute.subtype"/>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes the element in some sense, using any convenient classification scheme or typology.</a:documentation>
            <choice>
               <value>person</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks a person that is not explicitly mentioned by name.</a:documentation>
               <value>persons</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks persons that are not explicitly mentioned by name.</a:documentation>
               <value>letter</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks a letter that is not explicitly entitled.</a:documentation>
               <value>letters</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks letters that are not explicitly entitled.</a:documentation>
               <value>diaryDay</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks a dairy entry that is not explicitly mentioned.</a:documentation>
               <value>diaryDays</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks dairy entries that are not explicitly mentioned.</a:documentation>
               <value>work</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks a work that is not explicitly mentioned.</a:documentation>
               <value>works</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks works that are not explicitly mentioned.</a:documentation>
               <value>writing</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks a writing that is not explicitly mentioned.</a:documentation>
               <value>writings</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks writings that are not explicitly mentioned.</a:documentation>
               <value>news</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks news that are not explicitly mentioned.</a:documentation>
               <value>biblio</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks bilbiographic entries that are not explicitly mentioned.</a:documentation>
               <value>org</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks an organization that is not explicitly mentioned.</a:documentation>
               <value>orgs</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks organizations that are not explicitly mentioned.</a:documentation>
               <value>place</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks a place that is not explicitly mentioned.</a:documentation>
               <value>places</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks places that are not explicitly mentioned.</a:documentation>
               <value>document</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks a document that is not explicitly entitled.</a:documentation>
               <value>documents</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks organizations that are not explicitly mentioned.</a:documentation>
               <value>mixed</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks a variety of documents/entities that are not explicitly mentioned.</a:documentation>
               <value>addenda</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks a reference to addenda of printed volumes.</a:documentation>
            </choice>
         </attribute>
         <optional>
            <attribute name="key">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">provides an externally-defined means of identifying the entity (or entities) being named, using a coded value of some kind.</a:documentation>
               <list>
                  <oneOrMore>
                     <choice>
                        <data type="string">
                           <param name="pattern">A00[0-9A-F]{4}</param>
                        </data>
                        <data type="string">
                           <param name="pattern">A02\d{4}</param>
                        </data>
                        <data type="string">
                           <param name="pattern">A03\d{4}</param>
                        </data>
                        <data type="string">
                           <param name="pattern">A04\d{4}</param>
                        </data>
                        <data type="string">
                           <param name="pattern">A05\d{4}</param>
                        </data>
                        <data type="string">
                           <param name="pattern">A06\d{4}</param>
                        </data>
                        <data type="string">
                           <param name="pattern">A07\d{4}</param>
                        </data>
                        <data type="string">
                           <param name="pattern">A08\d{4}</param>
                        </data>
                        <data type="string">
                           <param name="pattern">A10\d{4}</param>
                        </data>
                        <data type="string">
                           <param name="pattern">A11\d{4}</param>
                        </data>
                        <data type="string">
                           <param name="pattern">A13\d{4}</param>
                        </data>
                        <data type="string">
                           <param name="pattern">A22\d{4}</param>
                        </data>
                        <data type="string">
                           <param name="pattern">A12\d{4}</param>
                        </data>
                     </choice>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <ref name="att.global.responsibility.attribute.cert"/>
         <empty/>
      </element>
   </define>
   <define name="address">
      <element name="address">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(address) contains a postal address, for example of a publisher, an organization, or an individual. [3.6.2. Addresses 2.2.4. Publication, Distribution, Licensing, etc. 3.12.2.4. Imprint, Size of a Document, and Reprint Information]</a:documentation>
         <oneOrMore>
            <ref name="addrLine"/>
         </oneOrMore>
      </element>
   </define>
   <define name="addrLine">
      <element name="addrLine">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(address line) contains one line of a postal address. [3.6.2. Addresses 2.2.4. Publication, Distribution, Licensing, etc. 3.12.2.4. Imprint, Size of a Document, and Reprint Information]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <optional>
            <attribute name="n">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(number) gives a number (or other label) for an element, which is not necessarily unique within the document.</a:documentation>
               <choice>
                  <value>telephone</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">telephone number of the relevant correspondent in the address line.</a:documentation>
                  <value>email</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">email of the relevant correspondent in the address line.</a:documentation>
                  <value>fax</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">fax number of the relevant correspondent in the address line.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="date">
      <element name="date">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(date) contains a date in any format. [3.6.4. Dates and Times 2.2.4. Publication, Distribution, Licensing, etc. 2.6. The Revision Description 3.12.2.4. Imprint, Size of a Document, and Reprint Information 16.2.3. The Setting Description 14.4. Dates]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="model.gLike"/>
               <ref name="model.phrase"/>
               <ref name="model.global"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-date-dateHeuristics2-constraint-rule-36">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:date[@notBefore castable as xs:date and @notAfter castable as xs:date]">
               <sch:report test="xs:date(@notBefore) gt xs:date(@notAfter)">the date value of notBefore is later than notAfter</sch:report>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:date[@notBefore castable as xs:gYear and @notAfter castable as xs:gYear]">
               <sch:report test="xs:date(concat(@notBefore, '-01-01')) gt xs:date(concat(@notAfter, '-01-01'))">the date value of notBefore is later than notAfter</sch:report>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:date[@notBefore castable as xs:gYear and @notAfter castable as xs:date]">
               <sch:report test="xs:date(concat(@notBefore, '-01-01')) gt xs:date(@notAfter)">the date value of notBefore is later than notAfter</sch:report>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:date[@notBefore castable as xs:date and @notAfter castable as xs:gYear]">
               <sch:report test="xs:date(@notBefore) gt xs:date(concat(@notAfter, '-12-31'))">the date value of notBefore is later than notAfter</sch:report>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-date-dateHeuristics3-constraint-rule-40">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:date">
               <sch:report test="(@from or @to) and (@when or @notAfter or @notBefore)">cooccurence of date and duration attributes</sch:report>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-date-dateHeuristics4-constraint-rule-41">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:date">
               <sch:report test="@when and (@notAfter or @notBefore)">cooccurence of exact and uncertain date attributes</sch:report>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-date-dateHeuristics5-constraint-rule-42">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:date[ancestor::tei:teiHeader]">
               <sch:assert test="@when or @notBefore or @notAfter or @from or @to or @cert='unknown' or .='o.J.' or .='o. J.'">tei:date benötigt ein Attribut @when, @notBefore o.ä.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-date-dateHeuristics6-constraint-rule-43">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:date[some $i in (@when, @notBefore, @notAfter, @from, @to) satisfies $i castable as xs:date]">
               <sch:report test="every $j in (@when, @notBefore, @notAfter, @from, @to)[. castable as xs:date] satisfies xs:date($j) gt current-date()">some attribute value of <sch:name/> is greater than the current date</sch:report>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:date[some $i in (@when, @notBefore, @notAfter, @from, @to) satisfies $i castable as xs:gYear]">
               <sch:report test="every $j in (@when, @notBefore, @notAfter, @from, @to)[. castable as xs:gYear] satisfies number($j) gt year-from-date(current-date())">some attribute value of <sch:name/> is greater than the current date</sch:report>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-date-julian-before-1582-constraint-rule-45">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:date[@calendar='Julian']">
               <sch:report test="some $i in (@when, @notBefore, @notAfter, @from, @to) satisfies $i lt '1582-10-15'">Before October 15, 1582 the default calendar is Julian so no extra calendar declaration is needed.</sch:report>
            </sch:rule>
         </pattern>
         <ref name="att.global.responsibility.attributes"/>
         <ref name="att.calendarSystem.attributes"/>
         <ref name="att.datable.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes the element in some sense, using any convenient classification scheme or typology.</a:documentation>
               <choice>
                  <value>baptism</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Baptism) Identifies the selected date as date of baptism.</a:documentation>
                  <value>funeral</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Funeral) Identifies the selected date as date of funeral.</a:documentation>
                  <value>performance</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Performance) Identifies the selected date as date of performance.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <empty/>
      </element>
   </define>
   <define name="ref">
      <element name="ref">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(reference) defines a reference to another location, possibly modified by additional text or comment. [3.7. Simple Links and Cross-References 17.1. Links]</a:documentation>
         <ref name="macro.paraContent"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-ref-prev-next-correspContext-constraint-rule-46">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:ref[not(parent::tei:correspContext)]">
               <sch:report test="@type=('previousLetterFromAddressee', 'nextLetterFromAddressee')">'previousLetterFromAddressee' and 'nextLetterFromAddressee' are only used within the correspContext element.</sch:report>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-ref-refAtts-constraint-rule-47">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:ref">
               <sch:report test="@target and @cRef">Only one of the attributes @target and @cRef may be supplied on <sch:name/>.</sch:report>
            </sch:rule>
         </pattern>
         <ref name="att.internetMedia.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes the element in some sense, using any convenient classification scheme or typology.</a:documentation>
               <choice>
                  <value>footnoteAnchor</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Marks the footnote sign within the text.</a:documentation>
                  <value>hyperLink</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Marks some passage within the text that acts as a hyperlink to an external resource on the internet.</a:documentation>
                  <value>previousLetterFromAddressee</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Marks the (or a) preceding letter in the <code xmlns="http://www.w3.org/1999/xhtml"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0">&lt;correspContext&gt;</code> element, i.e. the letter to which the current is the reply.</a:documentation>
                  <value>nextLetterFromAddressee</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Marks the (or a) following letter in the <code xmlns="http://www.w3.org/1999/xhtml"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0">&lt;correspContext&gt;</code> element, i.e. the letter which is a reply to the current.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <attribute name="target">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies the destination of the reference by supplying one or more URI References.</a:documentation>
            <list>
               <oneOrMore>
                  <data type="anyURI">
                     <param name="pattern">\S+</param>
                  </data>
               </oneOrMore>
            </list>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="list">
      <element name="list">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(list) contains any sequence of items organized as a list. [3.8. Lists]</a:documentation>
         <group>
            <zeroOrMore>
               <ref name="head"/>
            </zeroOrMore>
            <choice>
               <oneOrMore>
                  <ref name="item"/>
                  <optional>
                     <ref name="note"/>
                  </optional>
                  <optional>
                     <ref name="model.milestoneLike"/>
                  </optional>
               </oneOrMore>
               <oneOrMore>
                  <ref name="item"/>
               </oneOrMore>
            </choice>
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-list-gloss-list-constraint-rule-48">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:list">
               <sch:report test="./tei:label and not(@type='gloss')">Labels are only allowed in lists of type='gloss'</sch:report>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-list-gloss-list-must-have-labels-constraint-rule-49">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:list[@type='gloss']">
               <sch:assert test="tei:label">The content of a "gloss" list should include a sequence of one or more pairs of a label element followed by an item element</sch:assert>
            </sch:rule>
         </pattern>
         <ref name="att.typed.attribute.subtype"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(type) characterizes the element in some sense, using any convenient classification scheme or typology.</a:documentation>
               <choice>
                  <value>simple</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">list items are not numbered or bulleted.</a:documentation>
                  <value>gloss</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">each list item glosses some term or concept, which is given by a label element preceding the list item.</a:documentation>
                  <value>ordered</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">list items are numbered or lettered.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="item">
      <element name="item">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(item) contains one component of a list. [3.8. Lists 2.6. The Revision Description]</a:documentation>
         <ref name="macro.specialPara"/>
         <ref name="att.global.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="head">
      <element name="head">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(heading) contains any type of heading, for example the title of a section, or the heading of a list, glossary, manuscript description, etc. [4.2.1. Headings and Trailers]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="model.gLike"/>
               <ref name="model.phrase"/>
               <ref name="model.inter"/>
               <ref name="model.lLike"/>
               <ref name="model.global"/>
            </choice>
         </zeroOrMore>
         <ref name="att.placement.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes the element in some sense, using any convenient classification scheme or typology.</a:documentation>
               <choice>
                  <value>sub</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Marks a subtitle</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="rend">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(rendition) specifies the text alignment in the source.</a:documentation>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="note">
      <element name="note">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(note) contains a note or annotation. [3.9.1. Notes and Simple Annotation 2.2.6. The Notes Statement 3.12.2.8. Notes and Statement of Language 10.3.5.4. Notes within Entries]</a:documentation>
         <ref name="macro.specialPara"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-note-note-in-text-constraint-rule-50">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:note[not(parent::tei:org or parent::tei:person or parent::tei:notesStmt or parent::tei:biblStruct or parent::tei:imprint or parent::tei:place)]">
               <sch:assert test="@type and @resp and @xml:id">It is required to provide @type and @resp and @xml:id on note.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-note-thematicCom-target-constraint-rule-51">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:note[@type='thematicCom' or @target]">
               <sch:assert test="@type='thematicCom' and @target">You need to provide thematic commentary targets (Only @type='thematicCom' and @target go together!).</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-note-thematicCom-must-be-empty-constraint-rule-52">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:note[@type='thematicCom' or @target]">
               <sch:report test="normalize-space() ne''">Thematic commentaries must not contain any text.</sch:report>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-note-notesStmt-notes-constraint-rule-53">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:note[parent::tei:notesStmt]">
               <sch:assert test="@type=('editorial', 'summary', 'thematicCom', 'incipit', 'textConst')">Within the notesStmt only 'editorial', 'summary', 'textConst', or 'thematicCom' are allowed for @type on note.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-note-text-notes-constraint-rule-54">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:note[not(parent::tei:notesStmt)]">
               <sch:report test="@type=('editorial', 'summary', 'incipit')">The note types 'editorial', 'summary', or 'thematicCom' are not allowed within the text section but only in the notesStmt.</sch:report>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-note-structured-content-notes-constraint-rule-55">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:note[parent::tei:notesStmt][tei:p|tei:list|tei:table]">
               <sch:report test="text()[matches(., '\w')] or *[not(self::tei:p or self::tei:list or self::tei:table)]">There must be no mixed content when a tei:p, tei:list, or tei:table is present.</sch:report>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-note-initial-uppercase-commentaries-constraint-rule-56">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:note[@type='commentary'][not(*[1]/self::tei:q or *[1]/self::tei:quote)]">
               <sch:assert test="matches(replace(normalize-unicode(normalize-space(.), 'NFKD'),  '[\p{M}]', ''), '^[A-Z0-9]')">Commentary notes must start with an upper case letter.</sch:assert>
            </sch:rule>
         </pattern>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.change.attribute.change"/>
         <ref name="att.global.facs.attribute.facs"/>
         <ref name="att.global.rendition.attribute.rend"/>
         <ref name="att.written.attributes"/>
         <optional>
            <attribute name="resp">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(responsible party) indicates the agency responsible for the intervention or interpretation, for example an editor or transcriber.</a:documentation>
               <ref name="staffSignatures.list"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="target">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(link targets) holds the URIs (notated like "wega:A07xxxx") of the to be linked thematic commentaries (only applicable if <code xmlns="http://www.w3.org/1999/xhtml"
                        xmlns:rng="http://relaxng.org/ns/structure/1.0">@type</code>="thematicCom" is also present)</a:documentation>
               <list>
                  <oneOrMore>
                     <ref name="thematicCom.target"/>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes the element in some sense, using any convenient classification scheme or typology.</a:documentation>
               <choice>
                  <value>commentary</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(commentary) </a:documentation>
                  <value>definition</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Word definitions/Term explanations) words should stand between pointer and note that marking is possible.</a:documentation>
                  <value>textConst</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Text constitution) Comments to variants, writings or other corrections which can not be encoded with an app or subst etc. in the letter.</a:documentation>
                  <value>thematicCom</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Thematic commentary) A synoptical commentary that can be applied to several places within the edition.</a:documentation>
                  <value>summary</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(summary) A summary of the content of the text (only applicable in the <code xmlns="http://www.w3.org/1999/xhtml"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0">&lt;notesStmt&gt;</code>).</a:documentation>
                  <value>editorial</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(general remark) Some general remark about the file (only applicable in the <code xmlns="http://www.w3.org/1999/xhtml"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0">&lt;notesStmt&gt;</code>).</a:documentation>
                  <value>incipit</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(incipit) The text incipit of the letter (or other piece of correspondence).</a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="idno">
      <element name="idno">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(identifier) supplies any form of identifier used to identify some object, such as a bibliographic item, a person, a title, an organization, etc. in a standardized way. [14.3.1. Basic Principles 2.2.4. Publication, Distribution, Licensing, etc. 2.2.5. The Series Statement 3.12.2.4. Imprint, Size of a Document, and Reprint Information]</a:documentation>
         <choice>
            <ref name="pnd.pattern"/>
            <ref name="viaf.pattern"/>
            <ref name="bd.pattern"/>
            <ref name="wikidata.id.pattern"/>
            <ref name="geonames.pattern"/>
         </choice>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-idno-types-match-external-identifiers-constraint-rule-57">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xi="http://www.w3.org/2001/XInclude"
                      context="tei:idno[@type='gnd']">
               <sch:assert test="matches(., '^1[0123]?\d{7}[0-9X]|[47]\d{6}-\d|[1-9]\d{0,7}-[0-9X]|3\d{7}[0-9X]$')">That doesn't look like a valid GND ID.</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xi="http://www.w3.org/2001/XInclude"
                      context="tei:idno[@type='viaf']">
               <sch:assert test="matches(., '^[1-9]\d(\d{0,7}|\d{17,20})$')">That doesn't look like a valid VIAF ID.</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xi="http://www.w3.org/2001/XInclude"
                      context="tei:idno[@type='geonames']">
               <sch:assert test="matches(., '^[1-9][0-9]{0,8}$')">That doesn't look like a valid Geonames ID.</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xi="http://www.w3.org/2001/XInclude"
                      context="tei:idno[@type='bd']">
               <sch:assert test="matches(., '^BachDigitalPerson_agent')">That doesn't look like a valid Bach Digital ID.</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xi="http://www.w3.org/2001/XInclude"
                      context="tei:idno[@type='wikidata']">
               <sch:assert test="matches(., '^Q[1-9]\d*$')">That doesn't look like a valid Wikidata ID.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="wegaPersons-idno-match-external-identifiers-constraint-rule-62">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xi="http://www.w3.org/2001/XInclude"
                      context="tei:idno[parent::tei:place]">
               <sch:report test="@type=('bd')">Bach IDs are not meant for places.</sch:report>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns:mei="http://www.music-encoding.org/ns/mei"
                      xmlns:s="http://www.ascc.net/xml/schematron"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xi="http://www.w3.org/2001/XInclude"
                      context="tei:idno[parent::tei:person]">
               <sch:report test="@type=('geonames')">Geonames IDs are not meant for persons.</sch:report>
            </sch:rule>
         </pattern>
         <ref name="att.datable.attributes"/>
         <ref name="att.typed.attribute.subtype"/>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes the element in some sense, using any convenient classification scheme or typology.</a:documentation>
            <choice>
               <value>gnd</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Gemeinsame Normdatei) </a:documentation>
               <value>viaf</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Virtual International Authority File) </a:documentation>
               <value>bd</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Bach Digital Identifier) </a:documentation>
               <value>geonames</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">ID taken from http://www.geonames.org/</a:documentation>
               <value>wikidata</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Wikidata Q identifier</a:documentation>
            </choice>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="model.persNamePart">
