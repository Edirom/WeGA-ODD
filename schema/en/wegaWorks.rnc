      "@startid attribute\x{a}" ~
      "                should have content."
    ]
    "\x{a}" ~
    "              "
    sch:assert [
      role = "warning"
      test =
        "every $i in tokenize(., '\s+') satisfies substring($i,2)=//mei:*/@xml:id"
      "The\x{a}" ~
      "                value in @startid should correspond to the @xml:id attribute of an\x{a}" ~
      "                element."
    ]
    "\x{a}" ~
    "            "
  ]
  "\x{a}" ~
  "   "
]
